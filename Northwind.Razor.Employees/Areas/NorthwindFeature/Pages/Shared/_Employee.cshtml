@page
@using Northwind.Common.Models
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model Northwind.Razor.Employees.NorthwindFeature.Pages.Employees.EmployeesPageModel
<div class="row">
    <h1 class="display-2">Employees</h1>
</div>
<div class="row">
    @foreach (Employee employee in Model.Employees)
    {
        <div class="col-sm-3">
            <partial name="_Employee" model="employee" />
        </div>
    }
</div>